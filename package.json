{
  "name": "lpt-application-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --base=/lpt-ui/",
    "build:gcp": "npm run build -- --mode=gcp",
    "preview": "vite preview",
    "test": "vitest",
    "test:single": "npm test -- run",
    "test:cover": "npm test -- run --coverage.enabled true",
    "test:uat:dev": "start-server-and-test 'vite dev --port 4173 --mode=uat' http://localhost:4173 'cypress open --e2e'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:ci": "run-p lint:eslint lint:prettier lint:stylelint",
    "lint:eslint": "eslint ./**/*.{mjs,ts,vue}",
    "lint:prettier": "prettier --check  './**/*.{cjs,html,json,md,mjs,ts,vue}'",
    "lint:stylelint": "stylelint './**/*.vue'",
    "fix": "run-s fix:eslint fix:prettier fix:stylelint",
    "fix:eslint": "eslint --fix ./**/*.{cjs,mjs,ts,vue}",
    "fix:prettier": "prettier --write  './**/*.{cjs,html,json,md,mjs,ts,vue}'",
    "fix:stylelint": "stylelint --fix './**/*.vue'",
    "format": "run-s fix:prettier",
    "precommit": "npm-run-all fix:prettier --parallel type-check lint:eslint test:single"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "joi": "^17.13.3",
    "pinia": "^2.2.4",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.15.0",
    "cypress-log-to-output": "^1.1.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  }
}
